{
  "name": "marketplace2",
  "version": "1.0.0",
  "description": "E-commerce platform with REST API",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "test": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --runInBand --detectOpenHandles --verbose",
    "test:coverage": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --coverage --runInBand --detectOpenHandles --verbose",
    "test-coverage": "npm run test:coverage",
    "migration:generate": "node node_modules/typeorm/cli.js migration:generate ./migrations/InitialMigration -d config/data-source.js",
    "migration:run": "node node_modules/typeorm/cli.js migration:run -d config/data-source.js",
    "migration:revert": "node node_modules/typeorm/cli.js migration:revert -d config/data-source.js"
  },
  "keywords": [
    "nodejs",
    "express",
    "rest-api",
    "typeorm"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
